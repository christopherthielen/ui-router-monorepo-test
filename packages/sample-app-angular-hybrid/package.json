{
  "name": "sample-app-ng1-to-ng2",
  "description": "Architecture overview demo for Angular UI-Router",
  "version": "1.0.2",
  "scripts": {
    "start": "npm run watch:compile & npm run serve",
    "compile": "./node_modules/typescript/bin/tsc",
    "watch:compile": "./node_modules/typescript/bin/tsc -w",
    "serve": "webpack-dev-server --progress",
    "gh-pages": "git checkout gh-pages && git rebase master && rm -rf node_modules && npm install && echo 'If there are any updated modules:' && echo 'git commit -am update_libs' && echo 'git rebase -i' && echo && echo 'Finally:' && echo 'git push origin gh-pages:gh-pages --force' && echo 'git checkout master'"
  },
  "contributors": [
    {
      "name": "Chris Thielen",
      "web": "https://github.com/christopherthielen"
    }
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/ui-router/sample-app-ng1-to-ng2.git"
  },
  "dependencies": {
    "@angular/common": "^4.0.0",
    "@angular/compiler": "^4.0.0",
    "@angular/core": "^4.0.0",
    "@angular/forms": "^4.0.0",
    "@angular/http": "^4.0.0",
    "@angular/platform-browser": "^4.0.0",
    "@angular/platform-browser-dynamic": "^4.0.0",
    "@angular/router": "^4.0.0",
    "@angular/upgrade": "^4.0.0",
    "@uirouter/angular-hybrid": "^2.0.0",
    "@uirouter/visualizer": "^4.0.0",
    "angular": "<2",
    "es6-shim": "^0.35.0",
    "reflect-metadata": "0.1.2",
    "rxjs": "^5.0.0-beta.6",
    "zone.js": "^0.8.10"
  },
  "devDependencies": {
    "@types/angular": "^1.5.16",
    "@types/react": "^0.14.37",
    "@types/react-dom": "^0.14.17",
    "awesome-typescript-loader": "^3.1.3",
    "bootstrap": "^3.3.6",
    "d3": "^3.5.16",
    "font-awesome": "^4.5.0",
    "source-map-loader": "^0.2.1",
    "systemjs": "^0.19.24",
    "systemjs-plugin-css": "^0.1.20",
    "typescript": "^2.3.2",
    "webpack": "^2.5.1",
    "webpack-dev-server": "^2.4.5"
  }
}
