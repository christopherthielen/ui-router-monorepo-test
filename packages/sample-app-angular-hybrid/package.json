{
  "name": "sample-app-ng1-to-ng2",
  "description": "Architecture overview demo for Angular UI-Router",
  "version": "1.0.2",
  "scripts": {
    "start": "npm run serve",
    "serve": "webpack-dev-server --open --progress",
    "clean": "shx rm -rf _bundles",
    "build": "npm run clean && webpack",
    "test": "run-s build e2e",
    "e2e": "run-p -r browser-sync cypress",
    "browser-sync": "browser-sync --port 4000 --no-open",
    "cypress": "wait-on tcp:4000 && cypress run",
    "gh-pages": "git checkout gh-pages && git rebase master && rm -rf node_modules && yarn install && webpack && git add -f _bundles && git commit -m 'publish gh-pages' && git push origin gh-pages:gh-pages --force && git checkout master"
  },
  "contributors": [
    {
      "name": "Chris Thielen",
      "web": "https://github.com/christopherthielen"
    }
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/ui-router/sample-app-ng1-to-ng2.git"
  },
  "dependencies": {
    "@angular/common": "^4.4.4",
    "@angular/compiler": "^4.4.4",
    "@angular/core": "^4.4.4",
    "@angular/forms": "^4.4.4",
    "@angular/http": "^4.4.4",
    "@angular/platform-browser": "^4.4.4",
    "@angular/platform-browser-dynamic": "^4.4.4",
    "@angular/router": "^4.4.4",
    "@angular/upgrade": "^4.4.4",
    "@uirouter/angular-hybrid": "3.1.5",
    "@uirouter/visualizer": "^4.0.2",
    "angular": "<2",
    "rxjs": "5.4.3",
    "zone.js": "^0.8.10"
  },
  "devDependencies": {
    "@angular/compiler-cli": "4.4.4",
    "@ngtools/webpack": "1.7.2",
    "@types/angular": "^1.5.16",
    "@types/react": "^0.14.37",
    "@types/react-dom": "^0.14.17",
    "awesome-typescript-loader": "^3.1.3",
    "bootstrap": "^3.3.6",
    "browser-sync": "^2.21.0",
    "cypress": "^1.4.0",
    "d3": "^3.5.16",
    "font-awesome": "^4.5.0",
    "npm-run-all": "^4.1.2",
    "shx": "^0.2.2",
    "source-map-loader": "^0.2.1",
    "systemjs": "^0.19.24",
    "systemjs-plugin-css": "^0.1.20",
    "typescript": "^2.3.2",
    "wait-on": "^2.0.2",
    "webpack": "^2.5.1",
    "webpack-dev-server": "^2.4.5"
  }
}
