{
  "name": "@uirouter/sample-app-react",
  "version": "1.0.0",
  "description": "Architecture overview demo for UI-Router React",
  "scripts": {
    "start": "NODE_ENV=development webpack-dev-server --host 0.0.0.0 --port 3000 --config webpack.config.js --history-api-fallback --content-base ./",
    "build": "rimraf docs && mkdirp docs && cp index.html docs/index.html && NODE_ENV=production webpack",
    "test": "run-s build e2e",
    "test:ui": "run-s build && run-p -r browser-sync cypress:ui",
    "e2e": "run-p -r browser-sync cypress",
    "cypress": "wait-on tcp:4000 && cypress run",
    "cypress:ui": "wait-on tcp:4000 && cypress open",
    "browser-sync": "browser-sync start --port 4000 --server docs --no-open",
    "gh-pages": "npm run build && git add docs && git commit -m 'update github pages' docs && git push"
  },
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@uirouter/react": "0.6.0",
    "@uirouter/visualizer": "^5.0.6",
    "lodash": "^4.14.0",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "whatwg-fetch": "^2.0.3"
  },
  "devDependencies": {
    "autoprefixer": "^7.2.3",
    "babel-core": "^6.11.4",
    "babel-loader": "^7.1.2",
    "babel-plugin-syntax-trailing-function-commas": "^6.8.0",
    "babel-plugin-transform-class-properties": "^6.11.5",
    "babel-plugin-transform-object-rest-spread": "^6.8.0",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-es2016": "^6.11.3",
    "babel-preset-react": "^6.11.1",
    "browser-sync": "^2.21.0",
    "copy-webpack-plugin": "^4.3.1",
    "css-loader": "^0.28.7",
    "cypress": "^1.4.0",
    "file-loader": "^1.1.6",
    "npm-run-all": "^4.1.2",
    "postcss-load-config": "^1.2.0",
    "postcss-loader": "^2.0.6",
    "rimraf": "^2.5.4",
    "style-loader": "^0.19.1",
    "wait-on": "^2.0.2",
    "webpack": "^3.10.0",
    "webpack-dev-server": "^2.4.5"
  }
}
