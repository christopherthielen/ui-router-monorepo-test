{
  "name": "ui-router-react-hybrid",
  "description": "https://stackblitz.com/edit/ui-router-react-hybrid",
  "dependencies": {
    "@uirouter/react-hybrid": "0.1.0",
    "@uirouter/visualizer": "6.0.0",
    "angular": "1.6.9",
    "prop-types": "15.6.1",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-scripts": "1.1.1"
  },
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "eject": "react-scripts eject",
    "test": "run-s build e2e",
    "test-ui": "run-s build e2e-ui",
    "e2e": "run-p -r browser-sync cypress",
    "e2e-ui": "run-p -r browser-sync cypress-ui",
    "browser-sync": "browser-sync start --port 4000 --server build --no-open",
    "cypress": "wait-on tcp:4000 && cypress run",
    "cypress-ui": "wait-on tcp:4000 && cypress open"
  },
  "devDependencies": {
    "browser-sync": "^2.23.6",
    "cypress": "^2.1.0",
    "npm-run-all": "^4.1.2",
    "wait-on": "^2.1.0"
  }
}
